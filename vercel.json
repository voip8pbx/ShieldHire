{
    "version": 2,
    "functions": {
        "backend/api/index.ts": {
            "runtime": "@vercel/node",
            "includeFiles": "backend/src/**"
        }
    },
    "rewrites": [
        {
            "source": "/auth/:path*",
            "destination": "/backend/api/index.ts"
        },
        {
            "source": "/user/:path*",
            "destination": "/backend/api/index.ts"
        },
        {
            "source": "/bookings/:path*",
            "destination": "/backend/api/index.ts"
        },
        {
            "source": "/api/:path*",
            "destination": "/backend/api/index.ts"
        }
    ]
}